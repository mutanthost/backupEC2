# srdc_rman_restore_diag.rcv - collect RMAN information for restore/recover.
spool log to srdc_rman_restore_diag.out
set echo on
show all;
list incarnation of database;
report schema;
list backup summary;
list backup completed after 'sysdate-14';
list copy completed after 'sysdate-14';
restore database preview summary;
spool log off;
