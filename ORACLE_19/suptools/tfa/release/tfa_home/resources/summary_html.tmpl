        border-top-right-radius: 10px;
        border-bottom-right-radius: 10px;
        border-bottom-left-radius: 10px;
        border: 2px solid black;
      }
      .labeltab th:hover {
        background-color:white;
      }
      .labeltab th:focus {
        background-color:red;
      }
      .labeltab th {
        text-align: center;
        width:auto;
        height:5px;
        padding-top: 5px;
        padding-bottom: 5px;
        background-color:LightGrey ;
        padding-left: 5px;
        padding-right: 5px;
        border-top-left-radius: 5px;
        border-top-right-radius: 5px;
        border-bottom-right-radius: 5px;
        border-bottom-left-radius: 5px;
        border: 1px solid black;
      }
      .labeltable th a {
        display: inline-block;
        white-space: nowrap;
        text-align: center;
      }
      #content_bottom {
        background-image:url(images/main_bottom.png);
        background-repeat:no-repeat;
        width:100%;
        height:23px;
      }
      #footer {
        width:inherit;
        height:auto;
      }
      #footer h3 a,#footer h3 a:visited {
        display:inline;
        text-align:center;
        font-size:12px;
        text-decoration:none;
        color:#FFF;
      }
      #menu ul {
        list-style: none;
        padding: 0px;
        margin-left:auto;
        width:960px;
      }
      #menu li {
        list-style: none;
        padding: 0px;
        display: inline;
      }
      #menu a {
        float: left;
        height: 36px;
        display: block;
        text-align: center;
        text-decoration: none;
        color: #ffffff;
        font-weight: bold;
        padding-top: 18px;
        font-size: 15px;
        padding-left:13px;
        padding-right:13px;
      }
      #menu a:hover{
        background-image:url(images/link_background.png);
        background-repeat:repeat-x;
      }
      #to-top {
        position: fixed;
        bottom: 10px;
        right: 10px;
        width: 100px;
        padding: 5px;
        border: 1px solid #ccc;
        background: #f7f7f7;
        color: #333;
        text-align: center;
        cursor: pointer;
        display: none;
        #content p {
        }
        html, body {
          text-align: center;
        }
        p {
          text-align: left;
        }
        div.relative {
          position: relative;
          top: 30px;
          border: 3px solid #73AD21;
        }
        .lab td {
          text-align: center;
          padding: 5px 5px 5px 5px;
          background-color: #D3E1ED;
          border-top-right-radius: 12px;
          border: 1px solid black;
        }
    </style>
    <script type="text/javascript">
      function tabcolorChange(element,name,color,selectedcolor) {
        var str = "[id='" + name + "']";
        var elms = document.querySelectorAll(str);
        for(var i = 0; i < elms.length; i++) 
          elms[i].style.background=color;
        element.style.background=selectedcolor;
      }
      function showoverview(id) {
        document.getElementById(arguments[0]).style.display='none';
        document.getElementById(arguments[1]).style.display='block';
        var x = document.getElementById(arguments[1]).getElementsByTagName("td");
        for (i = 0; i < x.length; i++) {
          if(x[i].innerHTML == " FAIL " || x[i].innerHTML == " PROBLEM " ){
            x[i].bgColor = "tomato";
          }
        }
      }
      function showsuboverview(id) {
        for (i = 2; i < arguments.length; i++) {
          document.getElementById(arguments[i]).style.display='none';
        }
        document.getElementById(arguments[0]).style.display='block';
        document.getElementById(arguments[1]).style.display='block';
      }
      function showCollection(id) {
        document.getElementById("overview").style.display='none';
        //##component_left_menu_script##
        document.getElementById(id).style.display='block';
      }
      function showCollectionSubList(id) {
        if(document.getElementById(id).style.display == 'none'){
          document.getElementById(id).style.display='inline-block';
        }
        else {
          document.getElementById(id).style.display='none';
        }
      }
      function showMore(id) {
        var showID = id + "_show";
        var hideID = id + "_hide";
        if (document.getElementById(showID).style.display=='none') {
          document.getElementById(showID).style.display='block';
          document.getElementById(hideID).style.display='none';
        }
        else {
          document.getElementById(showID).style.display='none';
          document.getElementById(hideID).style.display='block';
        }
      }
      function hidetables() {
        var i;
        var component = arguments[0];
        var showID = arguments[1] + "_show";
        var hideID = arguments[1] + "_hide";
        for (i = 2; i < arguments.length; i++) {
          document.getElementById(arguments[i]).style.display='none';
        }
        document.getElementById(hideID).style.display='none';
        document.getElementById(showID).style.display='inline-block';
      }
      function showtables() {
        var i;
        var component = arguments[0];
        var showID = arguments[1] + "_show";
        var hideID = arguments[1] + "_hide";
        for (i = 2; i < arguments.length; i++) {
          document.getElementById(arguments[i]).style.display='inline-block';
        }
        document.getElementById(hideID).style.display='inline-block';
        document.getElementById(showID).style.display='none';
      }
      function showDiv(classname, value){
        var appBanners = document.getElementsByClassName(classname), i;
        for (var i = 0; i < appBanners.length; i ++) {
          appBanners[i].style.display = 'none';
        }
        var val = document.getElementById(value).value;
        document.getElementById(val).style.display = 'inline-block';
      }
    </script>
  </head>
  <body>
    <div id="container">
      <div id="header">
