<profile name="ologgerd_failures" description="For debugging ologgerd failure problems if directed by Oracle Support." type="default" timeout="10m">
<change>
<command>oclumon</command>
<command_location>GI_HOME</command_location>
<command_type>ologgerd</command_type>
<daemon>CRFLDREP</daemon>
<module>CRFLDREP</module>
<set>3</set>
<unset>1</unset>
</change>
<change>
<command>oclumon</command>
<command_location>GI_HOME</command_location>
<command_type>ologgerd</command_type>
<daemon>CRFLOGD</daemon>
<module>CRFLOGD</module>
<set>3</set>
<unset>1</unset>
</change>
<change>
<command>oclumon</command>
<command_location>GI_HOME</command_location>
<command_type>ologgerd</command_type>
<daemon>CRFM</daemon>
<module>CRFM</module>
<set>3</set>
<unset>1</unset>
</change>
</profile>

