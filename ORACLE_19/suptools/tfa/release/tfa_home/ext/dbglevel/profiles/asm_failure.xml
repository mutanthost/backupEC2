<profile name="asm_failure" description="For debugging general ASM failures if directed by Oracle Support" type="default" timeout="5m">
<change>
<command>crsctl</command>
<command_location>GI_HOME</command_location>
<command_type>resource</command_type>
<daemon>ora.asm</daemon>
<module>ora.asm</module>
<set>5</set>
<unset>1</unset>
</change>
</profile>




