<profile name="asm_listener_problem" description="For debugging ASM listener problems if directed by Oracle Support." type="default" timeout="5m">
<change>
<command>crsctl</command>
<command_location>GI_HOME</command_location>
<command_type>resource</command_type>
<daemon>ora.ASMNET1LSNR_ASM.lsnr</daemon>
<module>ora.ASMNET1LSNR_ASM.lsnr</module>
<set>5</set>
<unset>1</unset>
</change>
</profile>




