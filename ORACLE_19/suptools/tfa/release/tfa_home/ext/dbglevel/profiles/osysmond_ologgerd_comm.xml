<profile name="osysmond_ologgerd_comm" description="For debugging Cluster Health Monitor osysmond and ologgerd process communication problems if directed by Oracle Support." type="default" timeout="2m">
<change>
<command>crsctl</command>
<command_location>GI_HOME</command_location>
<command_type>log</command_type>
<daemon>gpnp</daemon>
<module>GIPC</module>
<set>3</set>
<unset>1</unset>
</change>
<change>
<command>crsctl</command>
<command_location>GI_HOME</command_location>
<command_type>log</command_type>
<daemon>crf</daemon>
<module>CRFLOGD</module>
<set>3</set>
<unset>1</unset>
</change>
<change>
<command>crsctl</command>
<command_location>GI_HOME</command_location>
<command_type>log</command_type>
<daemon>crf</daemon>
<module>CRFM</module>
<set>3</set>
<unset>1</unset>
</change>
<change>
<command>crsctl</command>
<command_location>GI_HOME</command_location>
<command_type>log</command_type>
<daemon>crf</daemon>
<module>CRFMOND</module>
<set>3</set>
<unset>1</unset>
</change>
</profile>





