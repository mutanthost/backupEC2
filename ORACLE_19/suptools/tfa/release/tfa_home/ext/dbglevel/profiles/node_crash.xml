<profile name="node_crash" description="For debugging node crashes if directed by Oracle Support." type="default" timeout="-1">
<change>
<command>crsctl</command>
<command_location>GI_HOME</command_location>
<command_type>resource</command_type>
<daemon>ora.cssd</daemon>
<module>ora.cssd</module>
<set>4 -init</set>
<unset>1 -init</unset>
</change>
<change>
<command>crsctl</command>
<command_location>GI_HOME</command_location>
<command_type>log</command_type>
<daemon>css</daemon>
<module>CLSF</module>
<set>4</set>
<unset>0</unset>
</change>
<change>
<command>crsctl</command>
<command_location>GI_HOME</command_location>
<command_type>log</command_type>
<daemon>css</daemon>
<module>CSSD</module>
<set>4</set>
<unset>2</unset>
</change>
<change>
<command>crsctl</command>
<command_location>GI_HOME</command_location>
<command_type>log</command_type>
<daemon>css</daemon>
<module>CSSDNMC</module>
<set>4</set>
<unset>2</unset>
</change>
</profile>



