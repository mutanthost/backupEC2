<profile name="scan_listener_problem" description="For debugging SCAN listener problems if directed by Oracle Support" type="default" timeout="5m">
<change>
<command>crsctl</command>
<command_location>GI_HOME</command_location>
<command_type>resource</command_type>
<daemon>ora.LISTENER_SCAN(n).lsnr</daemon>
<module>ora.LISTENER_SCAN(n).lsnr</module>
<set>5</set>
<unset>1</unset>
</change>
</profile>

