<profile name="vip_problem" description="For debugging VIP problems if directed by Oracle Support" type="default" timeout="-1">
<change>
<command>crsctl</command>
<command_location>GI_HOME</command_location>
<command_type>resource</command_type>
<daemon>ora.(hostname).vip</daemon>
<module>ora.(hostname).vip</module>
<set>5</set>
<unset>1</unset>
</change>
</profile>

