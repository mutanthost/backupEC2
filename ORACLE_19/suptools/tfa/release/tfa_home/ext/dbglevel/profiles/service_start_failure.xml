<profile name="service_start_failure" description="For debugging service start failure problems if directed by Oracle Support" type="default" timeout="5m">
<change>
<command>crsctl</command>
<command_location>GI_HOME</command_location>
<command_type>resource</command_type>
<daemon>ora.(dbname).srv</daemon>
<module>ora.(dbname).srv</module>
<set>5</set>
<unset>1</unset>
</change>
<change>
<command>crsctl</command>
<command_location>GI_HOME</command_location>
<command_type>resource</command_type>
<daemon>ora.(dbname).(service_name).svc</daemon>
<module>ora.(dbname).(service_name).svc</module>
<set>5</set>
<unset>1</unset>
</change>
<change>
<command>crsctl</command>
<command_location>GI_HOME</command_location>
<command_type>resource</command_type>
<daemon>ora.(dbname).(service_name).(instance_name).srv</daemon>
<module>ora.(dbname).(service_name).(instance_name).srv</module>
<set>5</set>
<unset>1</unset>
</change>
</profile>
