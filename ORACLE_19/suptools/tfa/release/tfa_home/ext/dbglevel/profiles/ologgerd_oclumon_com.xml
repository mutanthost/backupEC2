<profile name="ologgerd_oclumon_com" description="For debugging ologgerd oclumon communication failure problems if directed by Oracle Support." type="default" timeout="2m">
<change>
<command>oclumon</command>
<command_location>GI_HOME</command_location>
<command_type>ologgerd</command_type>
<daemon>CRFLOGD</daemon>
<module>CRFLOGD</module>
<set>3</set>
<unset>1</unset>
</change>
<change>
<command>oclumon</command>
<command_location>GI_HOME</command_location>
<command_type>ologgerd</command_type>
<daemon>CRFM</daemon>
<module>CRFM</module>
<set>3</set>
<unset>1</unset>
</change>
<change>
<command>oclumon</command>
<command_location>GI_HOME</command_location>
<command_type>ini</command_type>
<daemon>OCLUMON</daemon>
<module>OCLUMON</module>
<set>3</set>
<unset>1</unset>
</change>
</profile>
