<profile name="oclumon_high_cpu" description="For debugging oclumon high cpu failure problems if directed by Oracle Support." type="default" timeout="1m">
<change>
<command>oclumon</command>
<command_location>GI_HOME</command_location>
<command_type>ini</command_type>
<daemon>OCLUMON</daemon>
<module>OCLUMON</module>
<set>3</set>
<unset>1</unset>
</change>
</profile>
