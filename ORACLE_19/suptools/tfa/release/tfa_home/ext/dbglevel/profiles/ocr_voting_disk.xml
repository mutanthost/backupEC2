<profile name="ocr_voting_disk" description="For debugging OCR and/or Voting Disk problems if directed by Oracle Support." type="default" timeout="-1">
<change>
<command>crsctl</command>
<command_location>GI_HOME</command_location>
<command_type>log</command_type>
<daemon>crs</daemon>
<module>OCRASM</module>
<set>3</set>
<unset>1</unset>
</change>
<change>
<command>crsctl</command>
<command_location>GI_HOME</command_location>
<command_type>log</command_type>
<daemon>crs</daemon>
<module>OCRMAS</module>
<set>3</set>
<unset>1</unset>
</change>
<change>
<command>crsctl</command>
<command_location>GI_HOME</command_location>
<command_type>log</command_type>
<daemon>crs</daemon>
<module>OCRMSG</module>
<set>3</set>
<unset>1</unset>
</change>
<change>
<command>crsctl</command>
<command_location>GI_HOME</command_location>
<command_type>log</command_type>
<daemon>crs</daemon>
<module>OCRUTL</module>
<set>3</set>
<unset>1</unset>
</change>
</profile>




