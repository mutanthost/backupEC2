<profile name="database_resource_failure" description="For debugging database resources failure problems if directed by Oracle Support" type="default" timeout="-1">
<change>
<command>crsctl</command>
<command_location>GI_HOME</command_location>
<command_type>resource</command_type>
<daemon>ora.(dbname).db</daemon>
<module>ora.(dbname).db</module>
<set>5</set>
<unset>1</unset>
</change>
</profile>