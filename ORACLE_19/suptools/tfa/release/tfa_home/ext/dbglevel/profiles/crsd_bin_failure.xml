<profile name="crsd_bin_failure" description="For debugging crsd.bin failures if directed by Oracle Support." type="default" timeout="5m">
<change>
<command>crsctl</command>
<command_location>GI_HOME</command_location>
<command_type>log</command_type>
<daemon>crs</daemon>
<module>AGFW</module>
<set>5</set>
<unset>0</unset>
</change>
<change>
<command>crsctl</command>
<command_location>GI_HOME</command_location>
<command_type>log</command_type>
<daemon>crs</daemon>
<module>CRSPE</module>
<set>5</set>
<unset>0</unset>
</change>
<change>
<command>crsctl</command>
<command_location>GI_HOME</command_location>
<command_type>log</command_type>
<daemon>crs</daemon>
<module>UiServer</module>
<set>5</set>
<unset>0</unset>
</change>
</profile>




